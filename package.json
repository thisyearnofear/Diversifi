{
  "name": "ai-chatbot",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:diversifi": "pnpm --filter diversifi dev",
    "dev:packages": "pnpm --filter \"./packages/**\" dev",
    "build": "tsx scripts/deploy/build.ts",
    "build:all": "pnpm -r build",
    "build:packages": "pnpm --filter \"./packages/**\" build",
    "build:apps": "pnpm --filter \"./apps/**\" build",
    "build:diversifi": "pnpm --filter diversifi build",
    "start": "next start",
    "start:diversifi": "pnpm --filter diversifi start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "lint:all": "pnpm -r lint",
    "format": "biome format --write",
    "db:migrate": "tsx scripts/db/run-migrations.ts",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:seed": "pnpm tsx scripts/db/seed.ts",
    "db:seed:diversifi": "pnpm tsx scripts/db/seed-diversifi.ts",
    "fix:viem": "node scripts/utils/update-viem-version.js",
    "fix:tailwind": "node scripts/fixes/fix-tailwind-classes.js",
    "fix:web3-provider": "node scripts/fixes/fix-web3-provider.js",
    "fix:frame-paths": "node scripts/fixes/fix-frame-path-aliases.js",
    "fix:all": "node scripts/fixes/fix-all.js"
  },
